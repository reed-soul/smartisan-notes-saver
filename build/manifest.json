{
  "manifest_version": 3,
  "name": "锤子便签导出助手",
  "version": "0.2.1",
  "description": "一键导出所有锤子便签",
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://yun.smartisan.com/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "css": [
        "contentScript.css"
      ],
      "run_at": "document_start"
    }
  ],
  "host_permissions": [
    "*://yun.smartisan.com/*"
  ]
}
